generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id       String   @id @default(uuid())
  title    String
  value    Float
  type     String
  category String
  date     DateTime @default(now())

  @@map("transactions")
}
